{
  "name": "@recepturomat/ui",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "dependencies": {
    "@react-router/node": "^7.9.3",
    "@react-router/serve": "^7.9.3",
    "@tailwindcss/vite": "^4.1.8",
    "isbot": "^4.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.9.3",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.3",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "daisyui": "^5.0.43"
  },
  "nx": {
    "sourceRoot": "apps/ui",
    "targets": {
      "test": {
        "executor": "@nx/vitest:test",
        "options": {
          "testMode": "run",
          "configFile": "apps/ui/vite.config.ts",
          "passWithNoTests": true
        }
      },
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "apps/ui",
          "command": "npx react-router build"
        },
        "configurations": {
          "production": {
          },
          "dev": {
          }
        }
      },
      "serve": {
        "executor": "@nx/vite:dev-server",
        "options": {
          "configFile": "apps/ui/vite.config.ts",
          "env": {
            "BROWSER": "npx playwright open"
          }
        },
        "defaultConfiguration": "dev",
        "configurations": {
          "production": {
            "buildTarget": "@recepturomat/ui:build:production"
          },
          "dev": {
            "buildTarget": "@recepturomat/ui:build:dev"
          }
        }
      },
      "preview": {
        "executor": "@nx/vite:preview-server",
        "options": {
          "configFile": "apps/ui/vite.config.ts"
        },
        "defaultConfiguration": "production",
        "configurations": {
          "production": {
            "buildTarget": "@recepturomat/ui:build:production"
          }
        }
      },
      "typecheck": {
        "executor": "@nx/js:tsc",
        "options": {
          "tsConfig": "apps/ui/tsconfig.app.json"
        }
      }
    }
  }
}
