{
  "name": "@recepturomat/ui",
  "sourceRoot": "apps/ui",
  "targets": {
    "test": {
      "executor": "@nx/vitest:test",
      "options": {
        "configFile": "apps/ui/vite.config.ts",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "options": {
        "configFile": "apps/ui/vite.config.ts"
      }
    },
    "dev": {
      "executor": "@nx/vite:dev",
      "options": {
        "configFile": "apps/ui/vite.config.ts"
      }
    },
    "serve": {
      "executor": "@nx/vite:serve",
      "options": {
        "configFile": "apps/ui/vite.config.ts"
      }
    },
    "preview": {
      "executor": "@nx/vite:preview",
      "options": {
        "configFile": "apps/ui/vite.config.ts"
      }
    },
    "typecheck": {
      "executor": "@nx/js:tsc",
      "options": {
        "tsConfig": "apps/ui/tsconfig.app.json"
      }
    }
  }
}

