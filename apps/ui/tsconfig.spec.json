{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/vitest",
    "jsx": "react-jsx",
    "types": [
      "vitest/globals",
      "node",
      "@testing-library/jest-dom",
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts"
    ],
    "module": "ES2015",
    "moduleResolution": "bundler"
  },
  "files": ["src/test-setup.ts"],
  "include": [
    "vite.config.ts",
    "**/*.test.ts",
    "**/*.test.tsx",
    "src/**/*.d.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.app.json"
    }
  ]
}
